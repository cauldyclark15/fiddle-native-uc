{
  "plugins": [
    "transform-class-properties",
    "transform-es2015-modules-commonjs",
    [
      "module-resolver",
      {
        "alias": {
          "react-native": "react-native-web"
        },
        "extensions": [".web.js", ".mjs", ".js", ".json", ".web.jsx", ".jsx"]
      }
    ]
  ],
  "presets": ["react", "stage-2"]
}
